{
  "name": "edinet-financial-analyzer",
  "version": "1.0.0",
  "description": "EDINETデータを活用した有価証券報告書・四半期報告書の閲覧・分析アプリケーション",
  "type": "module",
  "main": "index.js",
  "engines": {
    "node": ">=24.0.0"
  },
  "scripts": {
    "start": "node --env-file=.env src/main.ts",
    "test": "vitest run --coverage",
    "format": "prettier -w .",
    "prepare": "husky"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/koumatsumoto/edinet-financial-analyzer.git"
  },
  "keywords": [
    "edinet",
    "financial",
    "securities",
    "xbrl",
    "analysis",
    "japan"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/koumatsumoto/edinet-financial-analyzer/issues"
  },
  "homepage": "https://github.com/koumatsumoto/edinet-financial-analyzer#readme",
  "devDependencies": {
    "@types/node": "^24.1.0",
    "@types/papaparse": "^5.3.16",
    "@vitest/coverage-v8": "^3.2.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.2",
    "msw": "^2.10.4",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "prettier": {
    "printWidth": 150
  },
  "lint-staged": {
    "**/*": [
      "prettier --write --ignore-unknown"
    ]
  },
  "dependencies": {
    "@types/jszip": "^3.4.0",
    "@types/uuid": "^10.0.0",
    "iconv-lite": "^0.6.3",
    "jszip": "^3.10.1",
    "papaparse": "^5.5.3",
    "uuid": "^11.1.0",
    "zod": "^4.0.16"
  }
}
